  <row>
    <panel>
      <title>Newly Discovered Domains</title>
      <table>
        <search>
          <query>host="censys_enterprise" type="DOMAIN" log_sub_type="add" 
          | table timestamp name 
          | sort -timestamp 
          | uniq 
          | rename name AS "Domain Name" 
          | rename timestamp AS Timestamp
          </query>
          <earliest>-30d@d</earliest>
          <latest>now</latest>
          <sampleRatio>1</sampleRatio>
        </search>
        <option name="count">10</option>
        <option name="drilldown">none</option>
        <option name="refresh.display">progressbar</option>
        <option name="wrap">false</option>        
      </table>
    </panel>
    <panel>
      <title>Domain Registrar Changes</title>
      <table>
        <search>
          <query>host="censys_enterprise" type="DOMAIN_REGISTRAR" 
          | table timestamp log_sub_type log_data.registrar name
          | sort -timestamp 
          | uniq 
          | rename timestamp AS Timestamp 
          | rename log_sub_type AS "Event Subtype"
          | rename log_data.registrar AS Registrar 
          | rename name AS "Domain Name" 
          </query>
          <earliest>-30d@d</earliest>
          <latest>now</latest>
          <sampleRatio>1</sampleRatio>
        </search>
        <option name="count">10</option>
        <option name="drilldown">none</option>
        <option name="refresh.display">progressbar</option>
        <option name="wrap">false</option>        
      </table>
    </panel>
  </row>
  <row>
    <panel>
